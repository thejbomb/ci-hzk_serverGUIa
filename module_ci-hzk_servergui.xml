<?xml version="1.0" encoding="UTF-8"?>
<project name="module_ci-hzk_servergui" default="compile.module.ci-hzk_servergui">
  <dirname property="module.ci-hzk_servergui.basedir" file="${ant.file.module_ci-hzk_servergui}"/>
  
  <property name="module.jdk.home.ci-hzk_servergui" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.ci-hzk_servergui" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.ci-hzk_servergui" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.ci-hzk_servergui" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="ci-hzk_servergui.output.dir" value="${module.ci-hzk_servergui.basedir}/out/production/ci-hzk_serverGUI"/>
  <property name="ci-hzk_servergui.testoutput.dir" value="${module.ci-hzk_servergui.basedir}/out/test/ci-hzk_serverGUI"/>
  
  <path id="ci-hzk_servergui.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="ci-hzk_servergui.module.production.classpath">
    <path refid="${module.jdk.classpath.ci-hzk_servergui}"/>
    <path refid="library.com.google.code.gson:gson:2.7.classpath"/>
  </path>
  
  <path id="ci-hzk_servergui.runtime.production.module.classpath">
    <pathelement location="${ci-hzk_servergui.output.dir}"/>
    <path refid="library.com.google.code.gson:gson:2.7.classpath"/>
  </path>
  
  <path id="ci-hzk_servergui.module.classpath">
    <path refid="${module.jdk.classpath.ci-hzk_servergui}"/>
    <pathelement location="${ci-hzk_servergui.output.dir}"/>
    <path refid="library.com.google.code.gson:gson:2.7.classpath"/>
  </path>
  
  <path id="ci-hzk_servergui.runtime.module.classpath">
    <pathelement location="${ci-hzk_servergui.testoutput.dir}"/>
    <pathelement location="${ci-hzk_servergui.output.dir}"/>
    <path refid="library.com.google.code.gson:gson:2.7.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.ci-hzk_servergui">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.ci-hzk_servergui">
    <patternset refid="excluded.from.module.ci-hzk_servergui"/>
  </patternset>
  
  <path id="ci-hzk_servergui.module.sourcepath">
    <dirset dir="${module.ci-hzk_servergui.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.ci-hzk_servergui" depends="compile.module.ci-hzk_servergui.production,compile.module.ci-hzk_servergui.tests" description="Compile module ci-hzk_serverGUI"/>
  
  <target name="compile.module.ci-hzk_servergui.production" depends="register.custom.compilers" description="Compile module ci-hzk_serverGUI; production classes">
    <mkdir dir="${ci-hzk_servergui.output.dir}"/>
    <javac2 destdir="${ci-hzk_servergui.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.ci-hzk_servergui}/javac">
      <compilerarg line="${compiler.args.ci-hzk_servergui}"/>
      <bootclasspath refid="ci-hzk_servergui.module.bootclasspath"/>
      <classpath refid="ci-hzk_servergui.module.production.classpath"/>
      <src refid="ci-hzk_servergui.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.ci-hzk_servergui"/>
    </javac2>
    
    <copy todir="${ci-hzk_servergui.output.dir}">
      <fileset dir="${module.ci-hzk_servergui.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.ci-hzk_servergui.tests" depends="register.custom.compilers,compile.module.ci-hzk_servergui.production" description="compile module ci-hzk_serverGUI; test classes" unless="skip.tests"/>
  
  <target name="clean.module.ci-hzk_servergui" description="cleanup module">
    <delete dir="${ci-hzk_servergui.output.dir}"/>
    <delete dir="${ci-hzk_servergui.testoutput.dir}"/>
  </target>
</project>